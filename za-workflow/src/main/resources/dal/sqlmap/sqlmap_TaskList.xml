  <insert id="insert" parameterClass="taskListDO" >
    INSERT INTO task_list( id, object_id, source, user_id, task_name, product_id, is_read, 
      complete_date, extra_info, is_deleted, gmt_created, creator, gmt_modified, 
      modifier)
     VALUES ( #id#, #objectId#, #source#, #userId#, #taskName#, #productId#, #isRead#, 
      #completeDate#, #extraInfo#, #isDeleted#, #gmtCreated#, #creator#, #gmtModified#, 
      #modifier#)
    <selectKey keyProperty="id" resultClass="java.lang.Long" >
      SELECT LAST_INSERT_ID() AS value
    </selectKey>
  </insert>

