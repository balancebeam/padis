                </column-validators>
            </column>
           <!-- 生效止期* -->
            <column index="8" name="expiryDate" type="java.util.Date" format="yyyy/MM/dd" disPlay="I列（到期日期）" >
            </column>            
            
		</data-schema>
	</message>
    
