  <insert id="insert" parameterClass="claimDocumentDO" >
    INSERT INTO group_claim_document( id, report_no, claim_id,insurance_certificate_id, liability_code, document_code, 
      document_info, remark, report_amount, reserve_amount, settlement_amount, 
      is_deleted, gmt_created, creator, gmt_modified, modifier,product_id,receipt_no)
     VALUES ( #id#, #reportNo#,#claimId#, #insuranceCertificateId#, #liabilityCode#, #documentCode#, 
      #documentInfo#, #remark#, #reportAmount#, #reserveAmount#, #settlementAmount#, 
      #isDeleted#, #gmtCreated#, #creator#, #gmtModified#, #modifier#,#productId#,#receiptNo#)
  </insert>

