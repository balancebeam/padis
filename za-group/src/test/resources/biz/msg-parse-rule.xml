<?xml version="1.0" encoding="UTF-8" ?>
<messageparser>

	<!-- 
	
	团险，被保人清单上传，对应excel清单的字段，对应policyInsurantDTO，顺序严格按照excel的列顺序
	二期的excel列顺序：
	0、姓名*	
	1、证件类型*	
	2、证件号码*	
	3、出生日期*	
	4、性别*    
	5、与主被保人关系            若为空，默认为 本人
	6、主被保人证件类型	
	7、主被保人证件号码 
	8、保险计划*
	9、生效日期               若为空，默认为 团单生效日期
	10、手机号	
	11、电子邮箱
	12、医保参保地
	13、社会保障号
	14、子公司	
	15、部门
	16、月工资	
	17、户籍	
	18、户口所在地	
	19、婚姻状况	
	20、民族	
	21、职业代码	
	22、职业类别	
	23、职业	
	24、理赔金转帐银行	
	25、户名	
	26、帐号	
	27、入司日期	
	28、服务年限	
	29、员工号	 
	30、 员工类别         若为空，默认为 在职		
	31、业务标识        若为空，默认为 新增
	32、	是否续保
	33、交易号
	34、备注
	带*号的为必填项
	      -->
    <message id="policyInsurantDTO">
        <data-schema>
           <!-- 姓名* -->
            <column index="0" name="insurantName" type="java.lang.String"  disPlay="A列（姓名）">
              <column-validators>
                    <column-validator not-empty="true" />
                </column-validators>
            </column>
            <!-- 证件类型* -->
            <column index="1" name="certiType" type="com.zhongan.core.user.constants.CusCertiType" str-to-enum="getEnumByNameWithNoDefault" disPlay="B列（证件类型）">
             <!--<column-validators>
                    <column-validator not-null="true" />
                </column-validators>-->
            </column>
            <!-- 证件号码* -->
            <column index="2" name="certiNo" type="java.lang.String" disPlay="C列（证件号码）">
            <!-- <column-validators>
                    <column-validator not-empty="true" />
                </column-validators>-->
            </column>
            <!-- 出生日期* -->
            <column index="3" name="birthday" type="java.util.Date" format="yyyyMMdd"  disPlay="D列（出生日期）">
            	 <column-validators>
                    <column-validator cus-validate-size="true" />
                </column-validators>
            </column>
            <!-- 性别* -->
            <column index="4" name="gender" type="com.zhongan.core.user.constants.CusUserGenderType" str-to-enum="getEnumByNameForGroup" disPlay="E列（性别）">
                <column-validators>
                    <column-validator not-null="true" />
                </column-validators>
            </column>
            <!-- 与 员工/主被保人 关系* -->
            <column index="5" name="relationToStaff" type="com.zhongan.core.user.constants.UserFamilyRelationEnum" str-to-enum="getEnum" disPlay="F列（与 主被保人 关系）">
            </column>
             <!-- 主被保人证件类型* -->
            <column index="6" name="baseCertiType" type="com.zhongan.core.user.constants.CusCertiType" str-to-enum="getEnumByName"  disPlay="G列（主被保人证件类型）">
            </column>
            <!-- 主被保人证件号码* -->
            <column index="7" name="baseCertiNo" type="java.lang.String" disPlay="H列（主被保人证件号码）" >
            </column>
             
             <!-- 保险计划* -->
            <column index="8" name="insurancePlan" type="java.lang.String" disPlay="I列（保险计划）">
                <column-validators>
                    <column-validator not-empty="true" />
                </column-validators>
            </column>
             <!-- 生效日期* -->
            <column index="9" name="effectiveDate" type="java.util.Date" format="yyyyMMdd" disPlay="J列（生效日期）" >
            </column>
           
			<!-- 手机号 -->
			<column index="10" name="phone" type="java.lang.String" disPlay="K列（手机号）" >
                  <column-validators>
                    <column-validator is-mobile-no="true" />
                  </column-validators>
            </column>
            <!-- 电子邮箱 -->
            <column index="11" name="email" type="java.lang.String"  disPlay="L列（电子邮箱）" >
                 <column-validators>
                    <column-validator is-email="true" />
                 </column-validators>
            </column>
           <!-- 医保参保地* -->
            <column index="12" name="medicalInsurancePlace" type="java.lang.String" disPlay="M列（医保参保地）">
            </column>
            <!-- 社会保障号 -->
            <column index="13" name="socialSecurityNo" type="java.lang.String" disPlay="N列（社会保障号）"></column>
            <!-- 子公司 -->
            <column index="14" name="subCompany" type="java.lang.String" disPlay="O列（子公司）"></column>
            <!-- 部门 -->
            <column index="15" name="department" type="java.lang.String" disPlay="P列（部门 ）"></column>
            <!-- 月工资 -->
            <column index="16" name="salary" type="java.lang.String" disPlay="Q列（月工资）"/>
            <!-- 户籍 -->
            <column index="17" name="nationality" type="java.lang.String" disPlay="R列（户籍）"></column>
            <!-- 户口所在地 -->
            <column index="18" name="householdLocation" type="java.lang.String" disPlay="S列（户口所在地）"></column>
            <!-- 婚姻状况 -->
            <column index="19" name="maritalStatus" type="java.lang.String" disPlay="T列（婚姻状况）"></column>
            <!-- 民族 -->
            <column index="20" name="nation" type="java.lang.String" disPlay="U列（民族）"></column>
            <!-- 职业代码 -->
            <column index="21" name="occupationalCode" type="java.lang.String" disPlay="V列（职业代码）"></column>
            <!-- 职业类别 -->
            <column index="22" name="occupationalType" type="java.lang.String" disPlay="W列（职业类别）"></column>
            <!-- 职业 -->
            <column index="23" name="position" type="java.lang.String" disPlay="X列（职业 ）"></column>
            <!-- 理赔金转帐银行 -->
            <column index="24" name="transferAccountBank" type="java.lang.String" disPlay="Y列（理赔金转帐银行）"/>
            <!-- 户名 -->
            <column index="25" name="accountHolder" type="java.lang.String" disPlay="Z列（户名）"></column>
            <!-- 帐号 -->
            <column index="26" name="accountNo" type="java.lang.String" disPlay="AA列（帐号）"></column>
            <!-- 入司日期 -->
            <column index="27" name="entryDate" type="java.util.Date" format="yyyyMMdd" disPlay="AB列（入司日期）"></column>
            <!-- 服务年限 -->
            <column index="28" name="workExperience" type="java.lang.Integer" disPlay="AC列（服务年限）"/>
            <!-- 员工号 -->
            <column index="29" name="staffNo" type="java.lang.String" disPlay="AD列（员工号）"></column>
             <!-- 员工类别* -->
            <column index="30" name="staffType" type="com.zhongan.core.user.constants.StaffTypeEnum" str-to-enum="getEnum" disPlay="AE列（员工类别）">
            </column>
             <!-- 业务标识* -->
            <column index="31" name="dataType" type="com.zhongan.core.group.constants.UploadDataTypeEnum" str-to-enum="getPolicyCustomerType" disPlay="AF列（业务标识）">
            </column>
            <!-- 是否续保 -->
             <column index="32" name="isRenewal" type="com.zhongan.core.group.constants.YesNoEnum" str-to-enum="getEnumByName" disPlay="AG列（是否续保）">
            </column>
            
            <!-- 标的属性 -->
            <column index="33" name="tradeNo" type="java.lang.String" disPlay="AH列（标的属性）"></column>
          
            <!-- 备注 -->
            <column index="34" name="remark" type="java.lang.String" disPlay="AI列（备注）"></column>
          
        </data-schema>
    </message>
    
    <message id="yuangongbaoInsuredDTO">
        <data-schema>
           <!-- 序号* -->
            <column index="0" name="indexPostion" type="java.lang.String"  disPlay="A列（序号）">
              <column-validators>
                    <column-validator not-empty="true" />
                </column-validators>
            </column>
            <!-- 姓名* -->
            <column index="1" name="insurantName" type="java.lang.String"  disPlay="B列（姓名）">
              <column-validators>
                    <column-validator max-length="32"/>
                </column-validators>
            </column>
            <!-- 证件类型* -->
            <column index="2" name="certiType" type="com.zhongan.core.user.constants.CusCertiType" str-to-enum="getEnumByNameForGWYGBGR" disPlay="C列（证件类型）">
             <column-validators>
                    <column-validator not-null="true" />
                </column-validators>
            </column>
            <!-- 证件号码* -->
            <column index="3" name="certiNo" type="java.lang.String" disPlay="D列（证件号码）">
             <column-validators>
                    <column-validator max-length="20"/>
                </column-validators>
            </column>
            <!-- 出生日期* -->
            <column index="4" name="birthday" type="java.util.Date" format="yyyy/MM/dd"  disPlay="E列（出生日期）">
            	 <column-validators>
                    <column-validator cus-validate-size="true" />
                </column-validators>
            </column>
            <!-- 性别* -->
            <column index="5" name="gender" type="com.zhongan.core.user.constants.CusUserGenderType" str-to-enum="getEnumByNameForGWYGBGR" disPlay="F列（性别）">
                <column-validators>
                    <column-validator not-null="true" />
                </column-validators>
            </column>
            <!-- 手机号 -->
			<column index="6" name="phone" type="java.lang.String" disPlay="G列（手机）" >
                  <column-validators>
                    <column-validator not-empty-mobile-no="true" />
                  </column-validators>
            </column>
         </data-schema>
    </message>
</messageparser>