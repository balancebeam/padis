<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">

	<bean id="openTairManager" class="com.zhongan.openapi.core.cache.OpenTairManager" init-method="init">
		<property name="configServerList">
             <list>
                 <value>${za.openapi.gw.tair.configserver1}</value>
                 <value>${za.openapi.gw.tair.configserver2}</value>
             </list>
        </property>
         <property name="groupName">
            <value>${za.openapi.gw.tair.groupname}</value>
        </property>
		<property name="namespace">
            <value>${za.openapi.gw.tair.namespace}</value>
        </property>
	</bean>

    <bean id="zaTairManager" class="com.taobao.tair.impl.DefaultTairManager" init-method="init">
		<property name="configServerList">
             <list>
                 <value>${za.openapi.gw.tair.configserver1}</value>
                 <value>${za.openapi.gw.tair.configserver2}</value>
             </list>
        </property>
   
         <property name="groupName">
            <value>${za.openapi.gw.tair.groupname}</value>
        </property>
           
	</bean>	
</beans>