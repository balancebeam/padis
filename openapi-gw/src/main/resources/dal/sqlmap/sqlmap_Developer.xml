<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

<sqlMap namespace="Developer" >

  <typeAlias alias="developerDO" type="com.zhongan.openapi.core.dal.dataobject.DeveloperDO" />
  <typeAlias alias="developerQuery" type="com.zhongan.openapi.core.dal.query.DeveloperQuery" />

  <resultMap id="developerDOResultMap" class="developerDO" >
    <result property="id" column="id" />
    <result property="devName" column="dev_name" />
    <result property="channelId" column="channel_id" />
    <result property="appKey" column="app_key" />
    <result property="status" column="status" />
    <result property="isDel" column="is_deleted" />
    <result property="gmtCreated" column="gmt_created" />
    <result property="gmtModified" column="gmt_modified" />
  </resultMap>

  <sql id="developer_query_columns" >
    a1.id, a1.dev_name, a1.channel_id, a1.app_key, a1.status, a1.is_deleted, a1.gmt_created, 
      a1.gmt_modified
  </sql>

  <sql id="queryQueryParams" >
    <dynamic prepend="WHERE" >
      <![CDATA[  
      is_deleted = 'N'
      ]]>
      <isParameterPresent prepend="AND">
	      <isNotNull property="id" prepend="AND" >
	        <![CDATA[  
	        id = #id#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="devName" prepend="AND" >
	        <![CDATA[  
	        dev_name = #devName#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="channelId" prepend="AND" >
	        <![CDATA[  
	        channel_id = #channelId#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="appKey" prepend="AND" >
	        <![CDATA[  
	        app_key = #appKey#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="status" prepend="AND" >
	        <![CDATA[  
	        status = #status#  
	        ]]>  
	      </isNotNull>
      </isParameterPresent>
    </dynamic>
  </sql>

  <insert id="insert" parameterClass="developerDO" >
    INSERT INTO developer( id, dev_name, channel_id, app_key, status, is_deleted, gmt_created, 
      gmt_modified)
     VALUES ( #id#, #devName#, #channelId#, #appKey#, #status#, 'N', now(), 
      now())
    <selectKey keyProperty="id" resultClass="java.lang.Long" >
      SELECT LAST_INSERT_ID() AS value
    </selectKey>
  </insert>

  <update id="update" parameterClass="developerDO" >
    UPDATE developer   
    	set gmt_modified = now()
      <isNotNull property="devName" prepend="," >
        <![CDATA[  
        dev_name = #devName#  
        ]]>  
      </isNotNull>
      <isNotNull property="channelId" prepend="," >
        <![CDATA[  
        channel_id = #channelId#  
        ]]>  
      </isNotNull>
      <isNotNull property="appKey" prepend="," >
        <![CDATA[  
        app_key = #appKey#  
        ]]>  
      </isNotNull>
      <isNotNull property="status" prepend="," >
        <![CDATA[  
        status = #status#  
        ]]>  
      </isNotNull>
     WHERE id = #id#  
  </update>

  <select id="findByPrimaryKey" parameterClass="java.lang.Long" resultMap="developerDOResultMap" >
     SELECT 
    <include refid="developer_query_columns" />
    <![CDATA[  
     FROM developer a1 WHERE id = #id#
    ]]>  
  </select>

  <delete id="deleteByPrimaryKey" parameterClass="java.lang.Long" >
     UPDATE developer set is_deleted = 'Y', gmt_modified = now() WHERE id = #id#
  </delete>

  <select id="countByDOExample" parameterClass="developerDO" resultClass="java.lang.Integer" >
    SELECT COUNT(id) AS num FROM developer 
    <include refid="queryQueryParams" />
  </select>

  <select id="findListByDO" resultMap="developerDOResultMap" parameterClass="developerDO" >
     SELECT 
    <include refid="developer_query_columns" />
     FROM developer a1 
    <include refid="queryQueryParams" />
  </select>

  <select id="countByQueryExample" parameterClass="developerQuery" resultClass="java.lang.Integer" >
    SELECT COUNT(id) AS num FROM developer 
    <include refid="queryQueryParams" />    
  </select>

  <select id="findListByQuery" resultMap="developerDOResultMap" parameterClass="developerQuery" >
     SELECT 
    <include refid="developer_query_columns" />
     FROM developer a1 
    <include refid="queryQueryParams" />
    <isNotNull property="start" >
		<isNotNull property="limit">
		     order by  gmt_created desc 
		   	limit  #start#,#limit#		
		</isNotNull>
    </isNotNull>    
  </select>
</sqlMap>