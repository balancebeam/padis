<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

<sqlMap namespace="PublicKey" >

  <typeAlias alias="publicKeyDO" type="com.zhongan.openapi.core.dal.dataobject.PublicKeyDO" />
  <typeAlias alias="publicKeyQuery" type="com.zhongan.openapi.core.dal.query.PublicKeyQuery" />

  <resultMap id="publicKeyDOResultMap" class="publicKeyDO" >
    <result property="id" column="id" />
    <result property="developerId" column="developer_id" />
    <result property="version" column="version" />
    <result property="effectDate" column="effect_date" />
    <result property="publicKey" column="public_key" />
    <result property="isDel" column="is_deleted" />
    <result property="gmtCreated" column="gmt_created" />
    <result property="gmtModified" column="gmt_modified" />
  </resultMap>

  <sql id="publicKey_query_columns" >
    a1.id, a1.developer_id, a1.version, a1.effect_date, a1.public_key, a1.is_deleted, a1.gmt_created, 
      a1.gmt_modified
  </sql>

  <sql id="queryQueryParams" >
    <dynamic prepend="WHERE" >
      <![CDATA[  
      is_deleted = 'N' 
      ]]>
      <isParameterPresent prepend="AND">
	      <isNotNull property="id" prepend="AND" >
	        <![CDATA[  
	        id = #id#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="developerId" prepend="AND" >
	        <![CDATA[  
	        developer_id = #developerId#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="version" prepend="AND" >
	        <![CDATA[  
	        version = #version#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="effectDate" prepend="AND" >
	        <![CDATA[  
	        effect_date = #effectDate#  
	        ]]>  
	      </isNotNull>
	      <isNotNull property="publicKey" prepend="AND" >
	        <![CDATA[  
	        public_key = #publicKey#  
	        ]]>  
	      </isNotNull>
      </isParameterPresent>
    </dynamic>
  </sql>

  <insert id="insert" parameterClass="publicKeyDO" >
    INSERT INTO public_key( id, developer_id, version, effect_date, public_key, is_deleted, gmt_created, 
      gmt_modified)
     VALUES ( #id#, #developerId#, #version#, #effectDate#, #publicKey#, 'N', now(), 
      now())
    <selectKey keyProperty="id" resultClass="java.lang.Long" >
      SELECT LAST_INSERT_ID() AS value
    </selectKey>
  </insert>

  <update id="update" parameterClass="publicKeyDO" >
    UPDATE public_key   set gmt_modified = now()  
      <isNotNull property="developerId" prepend="," >
        <![CDATA[  
        developer_id = #developerId#  
        ]]>  
      </isNotNull>
      <isNotNull property="version" prepend="," >
        <![CDATA[  
        version = #version#  
        ]]>  
      </isNotNull>
      <isNotNull property="effectDate" prepend="," >
        <![CDATA[  
        effect_date = #effectDate#  
        ]]>  
      </isNotNull>
      <isNotNull property="publicKey" prepend="," >
        <![CDATA[  
        public_key = #publicKey#  
        ]]>  
      </isNotNull>
     WHERE id = #id#  
  </update>

  <select id="findByPrimaryKey" parameterClass="java.lang.Long" resultMap="publicKeyDOResultMap" >
     SELECT 
    <include refid="publicKey_query_columns" />
    <![CDATA[  
     FROM public_key a1 WHERE id = #id#
    ]]>  
  </select>

  <delete id="deleteByPrimaryKey" parameterClass="java.lang.Long" >
     UPDATE public_key set is_deleted = 'Y', gmt_modified = now() WHERE id = #id#
  </delete>

  <select id="countByDOExample" parameterClass="publicKeyDO" resultClass="java.lang.Integer" >
    SELECT COUNT(id) AS num FROM public_key 
    <include refid="queryQueryParams" />
  </select>

  <select id="findListByDO" resultMap="publicKeyDOResultMap" parameterClass="publicKeyDO" >
     SELECT 
    <include refid="publicKey_query_columns" />
     FROM public_key a1 
    <include refid="queryQueryParams" />
  </select>

  <select id="countByQueryExample" parameterClass="publicKeyQuery" resultClass="java.lang.Integer" >
    SELECT COUNT(id) AS num FROM public_key 
    <include refid="queryQueryParams" />
  </select>

  <select id="findListByQuery" resultMap="publicKeyDOResultMap" parameterClass="publicKeyQuery" >
     SELECT 
    <include refid="publicKey_query_columns" />
     FROM public_key a1 
    <include refid="queryQueryParams" />
  </select>
</sqlMap>